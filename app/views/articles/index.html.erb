<div class="center">
	<div class="mylisttable">
		<table class="table">
  			<tr>
    			<th style="color: green;">Subject</th>
    			<th>Target</th>
			    <th style="color: orange;">Ä°n</th>
			    <th style="color: red;">Out</th>
			    <th style="color: white;">Date</th>
  			</tr>
 
 			 <% @articles.each do |article| %>
    		<tr>
			      <td><%= article.subject %></td>
			      <td><%= article.target %></td>
			      <td><%= link_to 'Come on!', article_path(article) %></td>
			      <td><%= link_to 'Delete!', article_path(article),
			              method: :delete,
			              data: { confirm: 'Delete?' } %></td>
			      <td><%= I18n.localize(article.date,format:"%d %B") %></td>
    		</tr>
  			<% end %>
  		</table>
	</div>
</div>